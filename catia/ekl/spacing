let root (VPMReference)
Let myPocketPS (AdvisorParameterSet)
Let myValue (LENGTH)

if(paramName->Search("r") < 0){ //ustawia parametr z
	myPocketPS = `Rzedy Lewa` 
}
else{
	myPocketPS = `Rzedy Prawa` 
}

