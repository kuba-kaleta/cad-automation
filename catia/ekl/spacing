	centerVB = centerofgravity(lineVB)
	`Relations\VB Scripts.1\VB Script.1` ->Run(ToString((FormatNumber(length(lineVB), "decimal", 4)->ToReal())*1000), point(centerVB->coord(1), centerVB->coord(2), centerVB->coord(3)))
}

